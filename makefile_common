OBJDIR = ../obj

xmlproxyDIR = ../xmlproxy
xmlproxySRC = TestNodeProxy
xmlproxyOBJ = $(addprefix $(OBJDIR)/,$(addsuffix .o,$(xmlproxySRC)))

LIBS = -lpthread -ldim -lxml2 -lxmlpreprocessor
LIBSDIR = -L$(DIMDIR)/linux -L../ -L$(XMLPROXY)
HDRDIR = -I$(DIMDIR)/dim -I$(XMLPROXY) -I../xmlproxy -I.. -I/usr/include/libxml2

#COMPILER
CFLAGS		= -O -Wall -fPIC -g3 -DXML=1
SOFLAGS		= -shared
CC			= g++
CCC			= gcc
